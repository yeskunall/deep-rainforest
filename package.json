{
  "name": "deep-rainforest",
  "displayName": "Deep Rainforest",
  "version": "0.3.0",
  "description": "Explore a deep rainforest of code with lush green hues and serene blue undertones with this VS Code theme.",
  "categories": [
    "Themes"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yeskunall/deep-rainforest"
  },
  "license": "MIT",
  "publisher": "yeskunall",
  "scripts": {
    "generate:theme": "ts-node source/index.ts"
  },
  "contributes": {
    "themes": [
      {
        "label": "Deep Rainforest Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/deep-rainforest-color-theme.json"
      }
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2
  },
  "dependencies": {
    "chroma-js": "^2.4.2"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.4.0",
    "@types/node": "^20.4.4",
    "@types/vscode": "^1.80.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "icon": "icon.png"
}
